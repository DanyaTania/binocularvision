<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd" version="2.3">
    <managed-bean>
        <managed-bean-name>AppItem</managed-bean-name>
        <managed-bean-class>by.gourianova.binocularvision.bean.AppItem</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <managed-bean>

        <managed-bean-name>AppsListController</managed-bean-name>
        <!-- <managed-bean-name>appcontroller</managed-bean-name>-->
        <managed-bean-class>by.gourianova.binocularvision.controller.AppsListController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <application>
        <locale-config>
            <default-locale>ja_JP</default-locale>

        </locale-config>



    </application>


</faces-config>